<h1> Add New Season</h1>

<%= form_for @season do |form| %>

  <div class="field">
    <%= form.label :title %>
    <br>
    <%= form.text_field :title, :placeholder => "enter value" %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <br>
    <%= form.text_field :description, :placeholder => "enter value" %>
  </div>
  <div class="formField">
  <%= form.label :year %>
  <br>
  <%= form.number_field :year, :value => 2020 %>
</div>


<div class="field">
    <%= form.submit %>
</div>

<% end %>